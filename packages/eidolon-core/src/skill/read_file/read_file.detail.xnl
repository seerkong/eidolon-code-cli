<type_def lang="typescript" #>

export interface ReadFileOuterInput {
    path: string;        // required
    startLine?: number;  // optional, 1-based inclusive
    endLine?: number;    // optional, inclusive (-1 for end)
    maxBytes?: number;   // optional, clamp output bytes
}
export type ReadFileOuterOutput = string;
export type read_file = (input: ReadFileOuterInput) => ReadFileOuterOutput;

